<div class="panel-heading list-header">
  <p class="list-title"><%= list.escape('title') %></p>
  <button class="btn-xs btn-info edit-list"><span class="glyphicon glyphicon-edit"></span></button>
  <button class="btn-xs btn-success expand-list"><span class="glyphicon glyphicon-menu-right"></span></button>
</div>

<% if (list.escape('description') !== "") { %>
  <div class="panel-body">
    <p class="list-desc"><%= list.escape('description') %></p>
  <% } %>
</div>

<ul class="list-group books-list"></ul>
