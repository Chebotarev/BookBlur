<div class="panel-heading">
  <p class="list-title"><%= list.escape('title') %></p>
  <button class="btn-xs btn-info expand-list glyphicon glyphicon-menu-right"></button>
</div>

<div class="panel-body">
  <% if (list.escape('description') === "") { %>
    <p class="list-desc"<i>No description</i></p>
  <% } else { %>
    <p class="list-desc"><%= list.escape('description') %></p>
  <% } %>
</div>
  <ul class="list-group books-list"></ul>
